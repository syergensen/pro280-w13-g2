<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <servlet>
        <servlet-name>ResultsServlet</servlet-name>
        <servlet-class>Servlet.ResultsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostGradServlet</servlet-name>
        <servlet-class>Servlet.PostGradServlet</servlet-class>
    </servlet>
    <welcome-file-list>
        <welcome-file>/start/start.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>403</error-code>
        <location>/WEB-INF/login/forbidden.jsp</location>
    </error-page>
</web-app>
